tmle_Y5_a1 = tmle_Y5_a1, tmle_Y6_a1 = tmle_Y6_a1,
tmle_Y0_a0 = tmle_Y0_a0, tmle_Y1_a0 = tmle_Y1_a0, tmle_Y2_a0 = tmle_Y2_a0, tmle_Y3_a0 = tmle_Y3_a0, tmle_Y4_a0 = tmle_Y4_a0,
tmle_Y5_a0 = tmle_Y5_a0, tmle_Y6_a0 = tmle_Y6_a0,
