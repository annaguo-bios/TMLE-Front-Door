log({tmle_Y_geq5_a1/(1-tmle_Y_geq5_a1)}/{tmle_Y_geq5_a0/(1-tmle_Y_geq5_a0)}),
log({tmle_Y_geq6_a1/(1-tmle_Y_geq6_a1)}/{tmle_Y_geq6_a0/(1-tmle_Y_geq6_a0)})
)
)
